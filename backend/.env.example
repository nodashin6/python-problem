# Judge System Environment Configuration
# ジャッジシステム環境設定

# 実行環境
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# サーバー設定
HOST=localhost
PORT=8901

# Supabase設定 (例) 
# 実際の値は各自の環境に合わせて設定してください
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# JWT設定
SECRET_KEY=your-super-secret-key-for-jwt-tokens
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=1440

# 実行制限
DEFAULT_TIME_LIMIT_MS=5000
DEFAULT_MEMORY_LIMIT_MB=256
DEFAULT_OUTPUT_LIMIT_KB=64

# ジャッジワーカー設定
MAX_CONCURRENT_JUDGES=3
JUDGE_TIMEOUT_SECONDS=30

# キャッシュ設定
CACHE_TTL_SECONDS=300
CACHE_MAX_SIZE=1000

# データベース設定 (必要に応じて) 
# DATABASE_URL=postgresql://user:password@localhost:5432/judge_system

# Redis設定 (必要に応じて) 
# REDIS_URL=redis://localhost:6379/0

# SMTP設定 (メール機能用、必要に応じて) 
# SMTP_HOST=smtp.gmail.com
# SMTP_PORT=587
# SMTP_USER=your_email@gmail.com
# SMTP_PASSWORD=your_app_password
